<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/registrationlayoutBackground"
        android:layout_width="match_parent"
        android:layout_height="@dimen/textview_or_edittext_hight_large"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/fellingReg_DeleteList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:background="@color/red">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:src="@mipmap/ic_delete_forever_white_24dp"
                android:textColor="@android:color/black"
                android:visibility="visible" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/fellingReg_UpdateList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:gravity="center"
                android:src="@mipmap/update"
                android:textColor="@android:color/black"
                android:visibility="visible" />
        </LinearLayout>

        <TextView
            android:id="@+id/fellingReg_SbblabelTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.15"
            android:gravity="center"
            android:text="SBB Label "
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_TreePartTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="T Part "
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_TreeNoTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="Tree No "
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_PlotNoTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="Plot No "
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_WSpiceTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="W-Spice"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_QualityTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="Quality "
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_DF1TxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="F 1"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_DF2TxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="F 2"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_DT1TxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="T 1"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_DT2TxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="T 2"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_LenghtTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="Length"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_NoteFTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.22"
            android:gravity="center"
            android:text="NF"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_NoteTTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.22"
            android:gravity="center"
            android:text="NT"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_NoteLTxT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.22"
            android:gravity="center"
            android:text="NL"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/fellingReg_VolumeTXT"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:gravity="center"
            android:text="Volume"
            android:textColor="@color/color_black"
            android:textStyle="bold" />

    </LinearLayout>

</LinearLayout>